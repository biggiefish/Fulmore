## Define Working Directory ---
setwd("C:/Users/evogt/R Analysis/EAV/GitHubMarkdown/Fulmore/docs/")
## Define and Load Input Files
#1. PacFish Temp and Flow Data
catch_dat <- "Data/RST Catch/RST_Catch_2024.04.17.xlsx"
